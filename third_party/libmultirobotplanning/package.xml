<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format2.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="2">
  <name>libmultirobotplanning</name>
  <version>0.0.2</version>
  <description>Library with search algorithms for task and path planning for multi robot/agent systems.</description>
  <author>Wolfgang Hoenig</author>

  <maintainer email="hoenig@tu-berlin.de">Wolfgang Hoenig</maintainer>
  <license>MIT</license>

  <buildtool_depend>cmake</buildtool_depend>

  <build_depend>boost</build_depend>
  <build_depend>pkg-config</build_depend>
  <build_depend>yaml-cpp</build_depend>
  <exec_depend>boost</exec_depend>
  <exec_depend>pkg-config</exec_depend>
  <exec_depend>yaml-cpp</exec_depend>

  <export>
    <build_type>cmake</build_type>
  </export>
</package>
